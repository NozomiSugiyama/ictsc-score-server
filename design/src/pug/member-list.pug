	.c-floating-action-buttons
		button.p-icon-button.p-icon-button_shape_circle.c-opened
			i.fa.fa-plus(aria-hidden='true')
		button.p-icon-button.p-icon-button_shape_circle
			i.fa.fa-chevron-up(aria-hidden='true')
		button.p-icon-button.p-icon-button_shape_circle.p-icon-button__label 参加者
		button.p-icon-button.p-icon-button_shape_circle.p-icon-button__label 運営
	.c-action-modal.c-action-modal_state_active.u-cf
		form(name='create-member', action='#')
			.u-flex
				.p-form-item.p-form-item_type_inline
					label(for='input-member-name') ユーザー名
					input#input-member-name(type='text', name='member-name')
				.p-form-item.p-form-item_type_inline
					label(for='input-member-id') メンバー ID
					input#input-member-id(type='text', name='member-id')
				.p-form-item.p-form-item_type_inline
					label(for='input-team-name') チーム名
					select#input-team-name(name='team-name')
						option(value='0') チーム名を選択してください
				.p-form-item.p-form-item_type_inline
					label(for='static-school-name') 所属
					p#static-school-name.p-supplement.p-supplement__important チーム名を選択すると自動表示されます
				.p-form-item.p-form-item_type_inline
					label(for='input-password') パスワード
					input#input-password(type='password', name='password')
				.p-form-item.p-form-item_type_inline
					label(for='select-role') ロール
					select#select-role(name='role')
						option(value='0') ロールを選択してください
			button.p-button.p-button_color_green.p-button_size_large.pull-right(type='submit') メンバー追加
